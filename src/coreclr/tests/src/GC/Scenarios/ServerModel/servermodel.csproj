<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <CLRTestExecutionArguments>/numrequests:100</CLRTestExecutionArguments>
    <GCStressIncompatible>true</GCStressIncompatible>
    <HeapVerifyIncompatible Condition="'$(ArchGroup)' == 'x86' or '$(ArchGroup)' == 'arm' or '$(ArchGroup)' == 'arm64'">true</HeapVerifyIncompatible>
  </PropertyGroup>
  <ItemGroup>
    <Compile Include="cache.cs" />
    <Compile Include="parameters.cs" />
    <Compile Include="request.cs" />
    <Compile Include="server.cs" />
    <Compile Include="servermodel.cs" />
    <Compile Include="settings.cs" />
  </ItemGroup>
</Project>
